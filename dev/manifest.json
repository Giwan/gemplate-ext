{
  "manifest_version": 2,

  "name": "Gemplate-ext",
  "description": "A basic template for ES6 and React based chrome extension.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
